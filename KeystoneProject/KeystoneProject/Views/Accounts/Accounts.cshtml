                                                    <div class="col-lg-4 col-md-6 col-sm-12 col-xs-12">
                                                        <label>Remark</label>
                                                    </div>
                                                    <div class="col-lg-8 col-md-6 col-sm-12 col-xs-12">
                                                        <textarea id="Remark" name="Remark" class="form-control"></textarea>
                                                    </div>
                                                </div>
                                                <!--TIN-->
                                                <div class="form-group col-lg-4 col-md-12 col-sm-12 col-xs-12">
                                                    <div class="col-lg-4 col-md-6 col-sm-12 col-xs-12">
                                                        <label>TIN</label>
                                                    </div>
                                                    <div class="col-lg-8 col-md-6 col-sm-12 col-xs-12">
                                                        <input type="text" id="TIN" name="TIN" class="form-control">
                                                    </div>
                                                </div>
                                                <!--TIN Date-->
                                                <div class="form-group col-lg-4 col-md-12 col-sm-12 col-xs-12">
                                                    <div class="col-lg-4 col-md-6 col-sm-12 col-xs-12">
                                                        <label>TIN Date</label>
                                                    </div>
                                                    <div class="col-lg-8 col-md-6 col-sm-12 col-xs-12">
                                                        <input type="date" id="TinDate" name="TinDate" value="@DateTime.Now.ToString("yyyy-MM-dd")" max="9999-12-31" class="form-control">
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                <!--Pan Number-->
                                                <div class="form-group col-lg-4 col-md-12 col-sm-12 col-xs-12">
                                                    <div class="col-lg-4 col-md-6 col-sm-12 col-xs-12">
                                                        <label>PAN No.</label>
                                                    </div>
                                                    <div class="col-lg-8 col-md-6 col-sm-12 col-xs-12">
                                                        <input type="text" id="PanNo" name="PanNo" class="form-control">
                                                    </div>
                                                </div>
                                                <!--GST Number-->
                                                <div class="form-group col-lg-4 col-md-12 col-sm-12 col-xs-12">
                                                    <div class="col-lg-4 col-md-6 col-sm-12 col-xs-12">
                                                        <label>GST No.</label>
                                                    </div>
                                                    <div class="col-lg-8 col-md-6 col-sm-12 col-xs-12">
                                                        <input type="text" id="GstNo" name="GstNo" class="form-control">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12"></div>
                            <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12" style="text-align: center; display: block;">
                                <div class="btn-group dropdown-split-warning">
                                    <button type="submit" class="btn btn-app" id="save">
                                        <i class="fa fa-save"></i>Save
                                    </button>
                                </div>
                                <div class="btn-group dropdown-split-warning">
                                    <a class="btn btn-app" id="Delete" onclick="Delete(document.getElementById('AccountsID').value)">
                                        <i class="fa fa-trash"></i>Delete
                                    </a>
                                </div>
                                <div class="btn-group dropdown-split-warning">
                                    <a class="btn btn-app" onclick="location.reload();">
                                        <i class="fa fa-refresh"></i>Clear
                                    </a>
                                </div>
                                <div class="btn-group dropdown-split-warning">
                                    <a class="btn btn-app" onclick="window.location.href='@Url.Action("PatientMaster", "Application")'">
                                        <i class="fa fa-sign-out"></i>Exit
                                    </a>
                                </div>
                                <div class="btn-group dropdown-split-warning">
                                    <a class="btn btn-app" id="accountsExcelExport"></a>
                                </div>
                            </div>
                            <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12"></div>
                        </div>

                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 margin-r-5" style=" padding-bottom: 10px;overflow:auto">
                                    <table id="accountsTableId" class="table table-bordered hover display dataTable table-hover" role="grid" aria-describedby="accounts_info"
                                           style="width: 100%">
                                        <thead style="background-color: #bddff7;">
                                            <tr>
                                                <th>Sr. no.</th>
                                                <th>Account Name</th>
                                                <th>Schedule</th>
                                                <th>Dr. Amt</th>
                                                <th>Cr .Amt</th>
                                                <th>Cr. Limit</th>
                                                <th>Print Name</th>
                                                <th>Locality</th>
                                                <th>Cr. Days</th>
                                                <th>Address</th>
                                                <th>Pin Code</th>
                                                <th>Phone No.</th>
                                                <th>Mobile No.</th>
                                                <th>Email Id</th>
                                                <th>Remark</th>
                                                <th>FAX</th>
                                                <th>Tin No.</th>
                                                <th>Pan No.</th>
                                                <th>Tin Date</th>
                                                <th>GST No.</th>
                                                <th style="display:none">AccountsID</th>
                                                <th style="display:none">OpeningBalance</th>
                                                <th style="display:none">City</th>
                                                <th style="display:none">State</th>
                                                <th style="display:none">Country</th>
                                                <th style="display:none">ScheduleID</th>
                                                <th style="display:none">CityID</th>
                                                <th style="display:none">StateID</th>
                                                <th style="display:none">CountryID</th>
                                                @*<th style="display:none">Print1</th>*@
                                            </tr>
                                        </thead>
                                        <tbody class="scrollbar"></tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
        </form>
    </div>
</body>
