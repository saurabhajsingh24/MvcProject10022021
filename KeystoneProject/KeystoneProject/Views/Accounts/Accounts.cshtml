<script>
    $(function () {
        $("#City").change(function () {
            debugger
            var City = $("#City").val();
            $.ajax({
                type: "POST",
                url: '@Url.Action("AjaxMethod", "Accounts")',
                data: '{City: ' + JSON.stringify(City) + '}',
                contentType: "application/json; charset=utf-8",
                success: function (searchList) {
                    debugger
                    $("#State").val(searchList[0]);
                    $("#Country").val(searchList[1]);
                    $("#StateID").val(searchList[2]);
                    $("#CountryID").val(searchList[3]);
                },
            });
        });
    });
</script>
<script type="text/javascript">
    $(document).ready(function () {
        debugger
        $.ajax({
            type: "GET",
            url: '@Url.Action("ShowAllAccounts", "Accounts")',
            dataType: "json",
            success: function (data) {
                debugger
                for (var i = 0; i < data.length; i++) {
                    var SrNo = i + 1;
                    $("#accountsTableId").append(
                        '<tr><td>' + SrNo + '</td><td>' + data[i].AccountName + '</td><td>' + data[i].Schedule + '</td><td>' + data[i].DrAmount + '</td><td>' + data[i].CrAmount + '</td><td>' + data[i].CrLimit + '</td><td>' + data[i].PrintName + '</td><td>' + data[i].Locality + '</td><td>' + data[i].CreditDays + '</td><td>' + data[i].Address + '</td><td>' + data[i].PinCode + '</td><td>' + data[i].PhoneNo + '</td><td>' + data[i].MobileNo + '</td><td>' + data[i].EmailID + '</td><td>' + data[i].Remark + '</td><td>' + data[i].Fax + '</td><td>' + data[i].TIN + '</td><td>' + data[i].PanNo + '</td><td>' + data[i].TinDate + '</td><td>' + data[i].GstNo + '</td><td style="display:none">' + data[i].AccountsID + '</td><td style="display:none">' + data[i].OpeningBalance + '</td><td style="display:none">' + data[i].City + '</td><td style="display:none">' + data[i].State + '</td><td style="display:none">' + data[i].Country + '</td><td style="display:none">' + data[i].ScheduleID + '</td><td style="display:none">' + data[i].CityID + '</td><td style="display:none">' + data[i].StateID + '</td><td style="display:none">' + data[i].CountryID + '</td></tr>');
                };
                $('#accountsTableId').DataTable({
                    "fixedHeader": true,
                    "paging": true,
                    "lengthChange": true,
                    "searching": true,
                    "ordering": true,
                    "info": true,
                    "autoWidth": true,
                    "responsive": false,
                });
                var buttons = new $.fn.dataTable.Buttons($('#accountsTableId').DataTable(), {
                    buttons: [
                        {
                            extend: 'excel',
                            text: '<li class="fa fa-file-excel-o fa-lg" style="font-weight: 600;"></li><br>Excel'
                        }
                    ]
                }).container().appendTo($('#accountsExcelExport'));
            },
            error: function (result) {
                alert("Error grd bind");
            }
        });
    });
</script>
<script>
    function Delete(AccountsID) {
        debugger
        if (AccountsID != "") {
            var ans = confirm("Are you sure you want to delete this Record?");
            if (ans) {
                $.ajax({
                    type: "POST",
                    url: '@Url.Action("DeleteAccount", "Accounts")',
                    data: { AccountsID: AccountsID },
                    dataType: "json",
                    success: function (data) {
                        debugger
                        if (data == "Account Deleted Successfully") {
                            alert("Account Deleted Successfully");
                            window.location.reload(true);
                        }
                        else {
                            alert(data);
                        }
                    },
                });
            }
        }
    }
</script>
