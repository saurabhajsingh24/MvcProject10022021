    });

    $("#PinCode").on("blur", function () {
        debugger
        var mobNum = $(this).val();
        var filter = /^\d*(?:\.\d{1,2})?$/;

        if (filter.test(mobNum)) {
            if (mobNum.length == 6) {
                //alert("valid");
