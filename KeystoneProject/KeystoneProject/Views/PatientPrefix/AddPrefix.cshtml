
@{
    ViewBag.Title = "AddPrefix";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

 

<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
@*<title>KeystoneHMS | Dashboard</title>*@
<!-- Tell the browser to be responsive to screen width -->
<meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
<!-- Bootstrap 3.3.7 -->
<link href="@Url.Content("~/Vendor/bower_components/bootstrap/dist/css/bootstrap.min.css")" rel="stylesheet">
<!-- Font Awesome -->
<link rel="stylesheet" href="@Url.Content("~/Vendor/bower_components/font-awesome/css/font-awesome.min.css")">
<!-- Ionicons -->
<link rel="stylesheet" href="@Url.Content("~/Vendor/bower_components/Ionicons/css/ionicons.min.css")">
<!-- jvectormap -->
<link rel="stylesheet" href="@Url.Content("~/Vendor/bower_components/jvectormap/jquery-jvectormap.css")">
<!-- Theme style -->
<link rel="stylesheet" href="@Url.Content("~/Vendor/dist/css/AdminLTE.min.css")">
<!-- AdminLTE Skins. Choose a skin from the css/skins
     folder instead of downloading all of them to reduce the load. -->
<link rel="stylesheet" href="@Url.Content("~/Vendor/dist/css/skins/_all-skins.min.css")">
<!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
<!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
<!--[if lt IE 9]>
<script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
<script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
<![endif]-->
<!-- Google Font -->
<link rel="stylesheet"
      href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">


<body class="hold-transition skin-blue sidebar-mini sidebar-collapse">
    <div class="wrapper">
    

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>Prefix</h1>
                <ol class="breadcrumb">
                    <li><a href="dashboard.html"><i class="fa fa-dashboard"></i> Home</a></li>
                    <li class="active"><a href="#">Prefix</a></li>
                </ol>
            </section>

            <!-- Main content -->
            <section class="content">
                <div class="row">
                    <!-- Registration Tab Starts -->
                    <div class="col-md-12">
                        <!-- general form elements -->
                        <div class="box box-primary">
                            <div class="box-header with-border">
                                <h3 class="box-title">Prefix</h3>
                            </div>
                            <!-- /.box-header -->
                            <!-- Registration Form Start -->
                            <div class="form-horizontal">
                                <div class="box-body">
                                    <!--Prefix UHID Start-->
                                    <div class="form-group">
                                        <label class="col-sm-2 control-label">UHID NO:-</label>
                                        <div class="col-sm-5">
                                            <input type="text" class="form-control" name="prefixUHID"
                                                   placeholder="Enter UHID prefix">
                                        </div>
                                        <div class="col-sm-5">
                                            <input type="number" class="form-control" name="UHIDNumber"
                                                   placeholder="Enter UHID number">
                                        </div>
                                    </div>
                                    <!--Prefix UHID End-->
                                    <!--Prefix IPD Start-->
                                    <div class="form-group">
                                        <label class="col-sm-2 control-label">IPD NO:-</label>
                                        <div class="col-sm-5">
                                            <input type="text" class="form-control" name="prefixIPD"
                                                   placeholder="Enter IPD prefix">
                                        </div>
                                        <div class="col-sm-5">
                                            <input type="number" class="form-control" name="IPDNumber"
                                                   placeholder="Enter IPD number">
                                        </div>
                                    </div>
                                    <!--Prefix IPD End-->
                                    <!--Prefix OPD Start-->
                                    <div class="form-group">
                                        <label class="col-sm-2 control-label">OPD NO:-</label>
                                        <div class="col-sm-5">
                                            <input type="text" class="form-control" name="prefixOPD"
                                                   placeholder="Enter OPD prefix">
                                        </div>
                                        <div class="col-sm-5">
                                            <input type="number" class="form-control" name="OPDNumber"
                                                   placeholder="Enter OPD number">
                                        </div>
                                    </div>
                                    <!--Prefix OPD End-->
                                    <!--Prefix Lab Start-->
                                    <div class="form-group">
                                        <label class="col-sm-2 control-label">Lab NO:-</label>
                                        <div class="col-sm-5">
                                            <input type="text" class="form-control" name="prefixLab"
                                                   placeholder="Enter Lab prefix">
                                        </div>
                                        <div class="col-sm-5">
                                            <input type="number" class="form-control" name="labNumber"
                                                   placeholder="Enter Lab number">
                                        </div>
                                    </div>
                                    <!--Prefix Lab End-->
                                    <!--Prefix IPD Bill Start-->
                                    <div class="form-group">
                                        <label class="col-sm-2 control-label">IPD Bill NO:-</label>
                                        <div class="col-sm-5">
                                            <input type="text" class="form-control" name="prefixIPDBill"
                                                   placeholder="Enter IPD Bill prefix">
                                        </div>
                                        <div class="col-sm-5">
                                            <input type="number" class="form-control" name="IPDBillNumber"
                                                   placeholder="Enter IPD Bill number">
                                        </div>
                                    </div>
                                    <!--Prefix IPD Bill End-->
                                    <!--Prefix Final Bill Start-->
                                    <div class="form-group">
                                        <label class="col-sm-2 control-label">Final Bill NO:-</label>
                                        <div class="col-sm-5">
                                            <input type="text" class="form-control" name="prefixFinalBill"
                                                   placeholder="Enter Final Bill prefix">
                                        </div>
                                        <div class="col-sm-5">
                                            <input type="number" class="form-control" name="finalBillNumber"
                                                   placeholder="Enter Final Bill number">
                                        </div>
                                    </div>
                                    <!--Prefix Final Bill End-->
                                    <!--Prefix OPD Bill Start-->
                                    <div class="form-group">
                                        <label class="col-sm-2 control-label">OPD Bill NO:-</label>
                                        <div class="col-sm-5">
                                            <input type="text" class="form-control" name="prefixOPDBill"
                                                   placeholder="Enter OPD Bill prefix">
                                        </div>
                                        <div class="col-sm-5">
                                            <input type="number" class="form-control" name="OPDBillNumber"
                                                   placeholder="Enter OPD Bill number">
                                        </div>
                                    </div>
                                    <!--Prefix OPD Bill End-->
                                    <!--Prefix Lab Bill Start-->
                                    <div class="form-group">
                                        <label class="col-sm-2 control-label">Lab Bill NO:-</label>
                                        <div class="col-sm-5">
                                            <input type="text" class="form-control" name="prefixLabBill"
                                                   placeholder="Enter Lab Bill prefix">
                                        </div>
                                        <div class="col-sm-5">
                                            <input type="number" class="form-control" name="labBillNumber"
                                                   placeholder="Enter Lab Bill number">
                                        </div>
                                    </div>
                                    <!--Prefix Lab Bill End-->
                                    <!--Prefix Provisional Bill Start-->
                                    <div class="form-group">
                                        <label class="col-sm-2 control-label">Provisional Bill NO:-</label>
                                        <div class="col-sm-5">
                                            <input type="text" class="form-control" name="prefixProvisionalBill"
                                                   placeholder="Enter Provisional Bill prefix">
                                        </div>
                                        <div class="col-sm-5">
                                            <input type="number" class="form-control" name="provisionalBillNumber"
                                                   placeholder="Enter Provisional Bill number">
                                        </div>
                                    </div>
                                    <!--Prefix Provisional Bill End-->
                                </div>
                                <!-- /.box-body -->
                            </div>
                            <!-- Registration Form Ends -->
                        </div>
                        <!-- /.box -->
                    </div>
                    <!-- Registration Tab Ends -->
                </div>
                <!-- /.row -->
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->


        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <!--<b>Version</b> 2.4.0-->
            </div>
            <div class="col-6">
                <!--<a href="#"><i class="fa fa-fw fa-save"></i>Save</a> &nbsp;&nbsp;&nbsp;-->
                <a class="btn btn-app">
                    <i class="fa fa-save"></i> Save
                </a>
                <a class="btn btn-app">
                    <i class="fa fa-repeat"></i> Refresh
                </a>
            </div>
            <!--<strong>Copyright &copy; 2014-2016 <a href="dashboard.html">Almsaeed Studio</a>.</strong> All rights
            reserved.-->
        </footer>

        <!-- Add the sidebar's background. This div must be placed immediately after the control sidebar -->
        <div class="control-sidebar-bg"></div>
    </div>
    
</body>
