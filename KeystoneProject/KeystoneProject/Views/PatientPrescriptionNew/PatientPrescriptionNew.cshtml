<script>
    $(document).ready(function () {
        $('input[name="selector"]').click(function () {
            alert($(this).val());
            var Number = $("#txtFollowUpDays").val();
            var d = "";
            var date = new Date();
            var radio = $(this).val();
            if (radio == "Days") {
                var days = parseInt(Number, 10);
                date.setDate(date.getDate() + parseInt(days));
            }
            else if (radio == "Month") {
                var month = parseInt(Number, 10);
                date.setMonth(date.getMonth() + month);
            }
            else if (radio == "Years") {
                var year = parseInt(Number, 10);
                date.setFullYear(date.getFullYear() + year);
            }
            d = date.getFullYear() + '-' + (date.getMonth() + 1) + '-' + date.getDate();
            alert(d);
            $.ajax({
                url: '@Url.Action("BindCaculteAge", "PatientPrescriptionNew")',
                type: "GET",
                data: { date: d },
                dataType: "json",
                success: function (data) {
                    $("#FollowUpDate").val(data);
                }
            })
            // year = "";
            //   if ($("#Year").val() == "" && $("#Month").val() == "" && $("#age_day").val() == "") {
            //     $("#FollowUpDate").val(date.toString("yyyy-MM-dd"));
            //  }
        });
    });
</script>
