                <div class="container-fluid">
                    <div class="row mb-2">
                        <div class="col-sm-6">
                            <h1>Bed Status</h1>
                        </div>
                        <div class="col-sm-6">
                            <ol class="breadcrumb float-sm-right">
                                <li class="breadcrumb-item"><a href="@Url.Action("Dashboard","Home")">Home</a></li>
                                <li class="breadcrumb-item"><a href="@Url.Action("Patient", "Application")">Patient Module</a></li>
                                <li class="breadcrumb-item active">Bed Status</li>
                            </ol>
                        </div>
                    </div>
                </div>
            </section>
            <section class="content">
                <div class="container-fluid">
                    <div class="row ">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="table-responsive">
                                <table id="example3" class="table table-bordered hover display dataTable table-hover" role="grid" style="width: 100%">
                                    <thead style="background-color: #bddff7;">
                                        <tr>
                                            <th>Ward Name</th>
                                            <th>Room Name</th>
                                            <th>Bed Number</th>
                                            <th>Admition Date</th>
                                            <th>Registration Date</th>
                                            <th>IPD No.</th>
                                            <th>Patient Name</th>
                                            <th>Consultant Doctor</th>
                                            <th>Refference Doctor</th>
                                            <th>Deposit</th>
                                            <th>TPA Name</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @{
                                            if (Model.dsView != null)
                                            {
                                                for (int i = 0; i < Model.dsView.Tables[0].Rows.Count; i++)
                                                {
                                                    string WardName = Model.dsView.Tables[0].Rows[i]["WardName"].ToString();
                                                    string RoomName = Model.dsView.Tables[0].Rows[i]["RoomName"].ToString();
                                                    string BedNO = Model.dsView.Tables[0].Rows[i]["BedNO"].ToString();
                                                    string AdmitDate = Model.dsView.Tables[0].Rows[i]["AdmitDate"].ToString();
                                                    string RegNO = Model.dsView.Tables[0].Rows[i]["RegNO"].ToString();
                                                    string IPDNO = Model.dsView.Tables[0].Rows[i]["IPDNO"].ToString();
                                                    string PatientName = Model.dsView.Tables[0].Rows[i]["PatientName"].ToString();
                                                    string ConsDoctor = Model.dsView.Tables[0].Rows[i]["ConsDoctor"].ToString();
                                                    string ReffDoctor = Model.dsView.Tables[0].Rows[i]["ReffDoctor"].ToString();
                                                    string Deposit = Model.dsView.Tables[0].Rows[i]["Deposit"].ToString();
                                                    string TPAName = Model.dsView.Tables[0].Rows[i]["TPA Name"].ToString();
                                                    <tr role="row" class="odd">
                                                        <td>@WardName</td>
                                                        <td>@RoomName</td>
                                                        <td>@BedNO</td>
                                                        <td>@AdmitDate</td>
                                                        <td>@RegNO</td>
                                                        <td>@IPDNO</td>
                                                        <td>@PatientName</td>
                                                        <td>@ConsDoctor</td>
                                                        <td>@ReffDoctor</td>
                                                        <td>@Deposit</td>
                                                        <td>@TPAName</td>
                                                    </tr>
                                                }
                                            }
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12" style="text-align: center;">
                            <div class="btn-group dropdown-split-warning">
                                <button type="submit" class="btn btn-app" id="save">
                                    <i class="fa fa-save"></i>Save
                                </button>
                            </div>
                            <div class="btn-group dropdown-split-warning">
                                <a class="btn btn-app">
                                    <i class="fa fa-trash"></i>Delete
                                </a>
                            </div>
                            <div class="btn-group dropdown-split-warning">
                                <a class="btn btn-app" onclick="location.reload();">
                                    <i class="fa fa-refresh"></i>Clear
                                </a>
                            </div>
                            <div class="btn-group dropdown-split-warning">
                                <a class="btn btn-app" onclick="window.location.href='@Url.Action("Patient", "Application")'">
                                    <i class="fa fa-sign-out"></i>Exit
                                </a>
                            </div>
                            <div class="btn-group dropdown-split-warning">
                                <a class="btn btn-app" id="example3ExcelExport"></a>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </div>
    </div>
</body>

