<script>
    $(document).ready(function () {
        debugger
        $("#taxInforamtionTypeTaxRate").change(function () {
            $("#taxInformationDetailTaxRate").val($("#taxInforamtionTypeTaxRate").val());
        });

        $("#taxInforamtionTypeTaxRate").change(function () {
            if (document.getElementById("interState").checked == false)
                $("#description").val($("#taxType").val() + " " + $("#taxInforamtionTypeTaxRate").val());
        });

        $("#taxType").change(function () {
            if (document.getElementById("interState").checked == false) {
                $("#description").val($("#taxType").val() + " " + $("#taxInforamtionTypeTaxRate").val());
            }
        });

        $("#interState").change(function () {
            if (document.getElementById("interState").checked == true) {
                document.getElementById("formType").disabled = false;
                $("#description").val("CST C FORM");
            }
            else
            {
                document.getElementById("formType").disabled = true;
                document.getElementById("interStateTaxRate").disabled = true;
            }
        });

        $("#formType").change(function () {
            if ($("#formType").val() == "C Form") {
                document.getElementById("interStateTaxRate").disabled = false;
            }
            else
            {
                document.getElementById("interStateTaxRate").disabled = true;
            }
        });

        $("#interStateTaxRate").change(function () {
            $("#taxInformationDetailTaxRate").val($("#interStateTaxRate").val());
        });

        $("#interStateTaxRate").change(function () {
            $("#taxInformationDetailTaxRate").val($("#interStateTaxRate").val());
        });

        $("#taxInformationDetailTaxRate").change(function () {
            document.getElementById("surcharge").disabled = false;
            document.getElementById("taxInformationDetailTaxAccount").disabled = false;
        });

        $("#surcharge").change(function () {
            document.getElementById("surchargeAccount").disabled = false;
        });

        $("#rateInclusiveInTax").change(function () {
            if (document.getElementById("rateInclusiveInTax").checked == true) {
                document.getElementById("taxInclusiveInRates").disabled = true;
                document.getElementById("taxInclusiveInRatesForLTE").disabled = true;
                document.getElementById("abatementRate").disabled = true;
            }
            else {
                document.getElementById("taxInclusiveInRates").disabled = false;
                document.getElementById("taxInclusiveInRatesForLTE").disabled = false;
                document.getElementById("abatementRate").disabled = false;
            }
        });

        $("#addCharge").click(function () {
           
            $("#before").removeAttr('disabled');
            $("#after").removeAttr('disabled');
        });

    });
</script>
<script>
    $(document).ready(function () {
        $('#taxInformationCompanyTable').delegate('td', 'click', function () {
            $("#save").html("<i class='fa fa-save'></i>Update");
            debugger;
            var row = $(this).parent().index();
            var taxInformationTypeID = document.getElementById("taxInformationCompanyTable").tBodies[0].rows[row].cells[6].innerHTML;
            $("#taxInformationTypeID").val(taxInformationTypeID);
            $.ajax({
                type: "GET",
                url: '@Url.Action("Rebind_Data", "TaxInformationCompany")',
                data: { taxInformationTypeID : taxInformationTypeID },
                dataType: "json",
                success: function (data) {
                    debugger;
                    $('#taxInformationTypeID').val(data[0].taxInformationTypeID);
                    $('#code').val(data[0].code);
                    $('#taxType').val(data[0].taxType);
                    $('#formType').val(data[0].formType);
                    $('#description').val(data[0].description);
                    $('#postingAccountID').val(data[0].postingAccountID);
                    $('#postingAccount').val(data[0].postingAccount);
                    $('#taxInformationDetailTaxRate').val(data[0].taxInformationDetailTaxRate);
                    $('#surcharge').val(data[0].surcharge);
                    $('#taxInclusiveInRates').val(data[0].taxInclusiveInRates);
                    $('#taxInformationDetailTaxAccountID').val(data[0].taxInformationDetailTaxAccountID);
                    $('#taxInformationDetailTaxAccount').val(data[0].taxInformationDetailTaxAccount);
                    $('#surchargeAccountID').val(data[0].surchargeAccountID);
                    $('#surchargeAccount').val(data[0].surchargeAccount);
                    $('#addCharge').val(data[0].addCharge);
                    $('#abatementRate').val(data[0].abatementRate);
                    $('#chargePromotionalRate').val(data[0].chargePromotionalRate);
                   
                    if (data[0].taxInformationType == "Sales") {
                        $('#sales').prop("checked", true);
                    }
                    else if (data[0].taxInformationType == "Purchase") {
                        $('#purchase').prop("checked", true);
                    }
                    else {
                        $('#sales').prop("checked", false);
                        $('#purchase').prop("checked", false);
                    }

                    if (data[0].discount == "Before") {
                        $('#before').prop("checked", true);
                        $("#before").removeAttr('disabled');
                        $("#after").removeAttr('disabled');
                    }
                    else if (data[0].discount == "After") {
                        $('#after').prop("checked", true);
                        $("#before").removeAttr('disabled');
                        $("#after").removeAttr('disabled');
                    }
                    else {
                        $('#before').prop("checked", false);
                        $('#after').prop("checked", false);
                    }

                    if (data[0].interState == "True") {
                        $('#interState').prop("checked", true);
                    }
                    else {
                        $('#interState').prop("checked", false);
                    }

                    if (data[0].rateInclusiveInTax == "True") {
                        $('#rateInclusiveInTax').prop("checked", true);
                    }
                    else {
                        $('#rateInclusiveInTax').prop("checked", false);
                    }

                    if (data[0].taxInclusiveInRatesForLTE == "True") {
                        $('#taxInclusiveInRatesForLTE').prop("checked", true);
                    }
                    else {
                        $('#taxInclusiveInRatesForLTE').prop("checked", false);
                    }

                    if (data[0].addICOnFreeGoods == "True") {
                        $('#addICOnFreeGoods').prop("checked", true);
                    }
                    else {
                        $('#addICOnFreeGoods').prop("checked", false);
                    }

                    if (data[0].calculateTaxBeforeDiscount == "True") {
                        $('#calculateTaxBeforeDiscount').prop("checked", true);
                    }
                    else {
                        $('#calculateTaxBeforeDiscount').prop("checked", false);
                    }

                    if (data[0].chargeTaxOnFreeGoods == "True") {
                        $('#chargeTaxOnFreeGoods').prop("checked", true);
                    }
                    else {
                        $('#chargeTaxOnFreeGoods').prop("checked", false);
                    }

                    if (data[0].taxOfFreeGoodsOnGrossValue == "True") {
                        $('#taxOfFreeGoodsOnGrossValue').prop("checked", true);
                    }
                    else {
                        $('#taxOfFreeGoodsOnGrossValue').prop("checked", false);
                    }

                    if (data[0].taxCalculateBasedOnMRP == "True") {
                        $('#taxCalculateBasedOnMRP').prop("checked", true);
                    }
                    else {
                        $('#taxCalculateBasedOnMRP').prop("checked", false);
                    }

                    if (data[0].abatementRebateForLTE == "True") {
                        $('#abatementRebateForLTE').prop("checked", true);
                    }
                    else {
                        $('#abatementRebateForLTE').prop("checked", false);
                    }

                    $('#taxInforamtionTypeTaxRate').val(data[0].taxInforamtionTypeTaxRate);
                    $('#interStateTaxRate').val(data[0].interStateTaxRate);

                }
            });
        })
    });
</script>
<script>
    $(document).ready(function () {
        debugger
        $("#postingAccount").autocomplete({
            source : function (request, response) {
                $.ajax({
                    type: "GET",
                    url: '@Url.Action("Bind_Posting_acc", "TaxInformationCompany")',
                    dataType: "json",
                    data: { prefix: request.term },
                    success: function (data) {
                        response($.map(data, function (item) {
                            return {
                                label: item.postingAccount, value: item.postingAccount, id: item.postingAccountID
                            };
                        }))
                    },
                })
            },
            minLength: 1,
            change: function (event, i) {
                debugger
                if (!i.item) {
                    this.value = '';
                    $("#postingAccount").val('');
                }
                $('#postingAccountID').val(i.item.id);
            }
        });
    });
</script>
