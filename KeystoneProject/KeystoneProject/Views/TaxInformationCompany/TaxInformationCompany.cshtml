                                                    <div class="form-check-inline">
                                                        <label class="form-check-label">
                                                            <input type="checkbox" class="form-check-input" id="taxInclusiveInRatesForLTE" name="taxInclusiveInRatesForLTE">Tax Inclusive In Rates For LTE
                                                        </label>
                                                    </div>
                                                </div>
                                                <!--Add I Charge-->
                                                <div class="form-group col-lg-3 col-md-12 col-sm-12 col-xs-12">
                                                    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                                                        <label>Add I Charge &#64;</label>
                                                    </div>
                                                    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                                                        <input type="number" id="addCharge" name="addCharge" value="0.00" class="form-control">
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                <!--Discount-->
                                                <div class="form-group col-lg-4 col-md-12 col-sm-12 col-xs-12">
                                                    <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                                                        <label>Discount</label>
                                                    </div>
                                                    <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                                        <div class="form-check-inline">
                                                            <label class="form-check-label">
                                                                <input type="radio" class="form-check-input" id="before" name="discount" value="Before" disabled="disabled">Before
                                                            </label>
                                                        </div>
                                                        <div class="form-check-inline">
                                                            <label class="form-check-label">
                                                                <input type="radio" class="form-check-input" id="after" name="discount" value="After" disabled="disabled">After
                                                            </label>
                                                        </div>
                                                    </div>
                                                </div>
                                                <!--Abatement Rate-->
                                                <div class="form-group col-lg-4 col-md-12 col-sm-12 col-xs-12">
                                                    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                                                        <label>Abatement Rate</label>
                                                    </div>
                                                    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                                                        <input type="number" id="abatementRate" name="abatementRate" value="0.00" class="form-control">
                                                    </div>
                                                </div>
                                                <!--Charge Promotional Rate-->
                                                <div class="form-group col-lg-4 col-md-12 col-sm-12 col-xs-12">
                                                    <div class="col-lg-5 col-md-5 col-sm-12 col-xs-12">
                                                        <label>Chrg. Promo. Rt.</label>
                                                    </div>
                                                    <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                                                        <input type="number" id="chargePromotionalRate" name="chargePromotionalRate" value="0.00" class="form-control">
                                                    </div>
                                                    <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
                                                        <label>% of MRP</label>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                <!--Add I. C. On Free Goods-->
                                                <div class="form-group col-lg-4 col-md-12 col-sm-12 col-xs-12">
                                                    <div class="form-check-inline">
                                                        <label class="form-check-label">
                                                            <input type="checkbox" class="form-check-input" id="addICOnFreeGoods" name="addICOnFreeGoods">Add I. C. On Free Goods
                                                        </label>
                                                    </div>
                                                </div>
                                                <!--Calculate Tax Before Discount-->
                                                <div class="form-group col-lg-4 col-md-12 col-sm-12 col-xs-12">
                                                    <div class="form-check-inline">
                                                        <label class="form-check-label">
                                                            <input type="checkbox" class="form-check-input" id="calculateTaxBeforeDiscount" name="calculateTaxBeforeDiscount">Calculate Tax Before Discount
                                                        </label>
                                                    </div>
                                                </div>
                                                <!--Charge Tax On Free Goods-->
                                                <div class="form-group col-lg-4 col-md-12 col-sm-12 col-xs-12">
                                                    <div class="form-check-inline">
                                                        <label class="form-check-label">
                                                            <input type="checkbox" class="form-check-input" id="chargeTaxOnFreeGoods" name="chargeTaxOnFreeGoods">Charge Tax On Free Goods
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                <!--Tax Of Free Goods On Gross Value-->
                                                <!--Charge Tax On Free Goods-->
                                                <div class="form-group col-lg-4 col-md-12 col-sm-12 col-xs-12">
                                                    <div class="form-check-inline">
                                                        <label class="form-check-label">
                                                            <input type="checkbox" class="form-check-input" id="taxOfFreeGoodsOnGrossValue" name="taxOfFreeGoodsOnGrossValue">Tax Of Free Goods On Gross Value
                                                        </label>
                                                    </div>
                                                </div>
                                                <!--Tax Calculate Based On MRP-->
                                                <div class="form-group col-lg-4 col-md-12 col-sm-12 col-xs-12">
                                                    <div class="form-check-inline">
                                                        <label class="form-check-label">
                                                            <input type="checkbox" class="form-check-input" id="taxCalculateBasedOnMRP" name="taxCalculateBasedOnMRP">Tax Calculate Based On MRP
                                                        </label>
                                                    </div>
                                                </div>
                                                <!--Abatement Rebate For LTE-->
                                                <div class="form-group col-lg-4 col-md-12 col-sm-12 col-xs-12">
                                                    <div class="form-check-inline">
                                                        <label class="form-check-label">
                                                            <input type="checkbox" class="form-check-input" id="abatementRebateForLTE" name="abatementRebateForLTE">Abatement Rebate For LTE
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12" style="text-align: center;">
                                <div class="btn-group dropdown-split-warning">
                                    <button type="submit" class="btn btn-app" id="save">
                                        <i class="fa fa-save"></i>Save
                                    </button>
                                </div>
                                <div class="btn-group dropdown-split-warning">
                                    <a class="btn btn-app" id="delete" onclick="Delete(document.getElementById('taxInformationTypeID').value); location.reload();">
                                        <i class="fa fa-trash"></i>Delete
                                    </a>
                                </div>
                                <div class="btn-group dropdown-split-warning">
                                    <a class="btn btn-app" onclick="location.reload();">
                                        <i class="fa fa-refresh"></i>Clear
                                    </a>
                                </div>
                                <div class="btn-group dropdown-split-warning">
                                    <a class="btn btn-app" onclick="window.location.href='@Url.Action("PatientMaster", "Application")'">
                                        <i class="fa fa-sign-out"></i>Exit
                                    </a>
                                </div>
                                <div class="btn-group dropdown-split-warning">
                                    <a class="btn btn-app" id="taxInformationCompanyTableExcelExport"></a>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="card-body col-md-12 col-sm-12 col-xs-12 table-responsive">
                                <table id="taxInformationCompanyTable" class="table table-bordered hover display dataTable table-hover" role="grid" style="width: 100%">
                                    <thead style="background-color: #bddff7;">
                                        <tr>
                                            <th>Sr. No.</th>
                                            <th>Code</th>
                                            <th>Tax Type Information</th>
                                            <th>Tax Type</th>
                                            <th>Account Name</th>
                                            <th>Tax Rate</th>
                                            <th style="display:none">Tax Type Information</th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
        </form>
    </div>
</body>