                        $("#patientsearch").append('<tr><td>' + data[i].RegNo + '</td><td>' + data[i].PatientName + '</td><td>' + data[i].PatientType + '</td><td>' + data[i].DoctorName + '</td><td>' + data[i].Date + '</td><td>' + data[i].BillAmt + '</td><td>' + data[i].consAmt + '</td><td>' + data[i].Discount + '</td><td>' + data[i].PaidAmount + '</td></tr>');
                    };



                }

            });
        })
    })
</script>


