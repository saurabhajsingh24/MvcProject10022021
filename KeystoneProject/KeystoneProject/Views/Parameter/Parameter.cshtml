                        <div class="row ">
                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 table-responsive">
                                <table id="parameterTableId" class="table table-bordered hover display dataTable table-hover" role="grid" style="width: 100%">
                                    <thead style="background-color: #bddff7;">
                                        <tr>
                                            <th style="display:none">ParameterID</th>
                                            <th>ParameterName</th>
                                            <th>PrintAs</th>
                                            <th>Alias</th>
                                            <th>Method</th>
                                            <th>Formula</th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                            </div>
                        </div>

                        <div id="myModal" class="modal fade" role="dialog">
                            <div class="modal-dialog modal-lg">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <h4 class="modal-title">Parameter Formula Detail</h4>
                                        <button type="button" id="btnclose" class="close" data-dismiss="modal">&times;</button>
                                    </div>
                                    <div class="modal-body">
                                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                            <table id="formulaTableId" class="table table-bordered hover display table-hover" role="grid" style="width: 100%">
                                                <thead style="background-color: #bddff7;">
                                                    <tr>
                                                        <th>Parameter Name</th>
                                                        <th>Ref. Name</th>
                                                        <th>Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody class="scrollbar">
                                                    <tr>
                                                        <td>
                                                            <input type="text" class="txtParameterFormula form-control" id="txtParameterFormula" name="txtParameterFormula">
                                                            <input type="hidden" id="ParameterFormulaID" name="ParameterFormulaID" />
                                                        </td>
                                                        <td>
                                                            <input type="text" class="form-control" id="txtReferenceName" name="txtReferenceName">
                                                        </td>
                                                        <td>
                                                            <span class="fa fa-trash buttoniconcss" id="deleteformulaTableId"></span>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 padding0">
                                            <div class="form-group col-lg-6 col-md-12 col-sm-12 col-xs-12 padding0">
                                                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                    <label>Short Name</label>
                                                </div>
                                                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                    <input type="text" id="txtShortName" name="txtShortName" class="form-control" style="width:100%">
                                                    <input type="hidden" id="ShortNameID" name="ShortNameID" />
                                                </div>
                                            </div>
                                            <div class="form-group col-lg-6 col-md-12 col-sm-12 col-xs-12 padding0">
                                                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                    <label>Full Name</label>
                                                </div>
                                                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                    <textarea id="txtFullName" rows="4" name="txtFullName" class="form-control" style="width:100%"></textarea>
                                                    <input type="hidden" id="FullNameID" name="FullNameID" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 padding0">
                                            <div class="form-group col-lg-6 col-md-12 col-sm-12 col-xs-12">
                                                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                    <button type="button" id="btnformulaAdd" class="btn btn-danger fa fa-eraser pull-right" title="Add"  >Add</button>
                                                </div>
                                            </div>
                                            <div class="form-group col-lg-6 col-md-12 col-sm-12 col-xs-12">
                                                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                    <input type="button" value="Ok" id="btnshow" class="btn btn-success pull-right" data-dismiss="modal" style="font-size: 15px;">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
        </form>
    </div>
