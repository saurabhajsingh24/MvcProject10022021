<script type="text/javascript">
    function ValidateEmail(EmailID) {
        var expr = /[A-Z0-9._%+-]+@@[A-Z0-9.-]+\.[A-Z]{2,4}/igm;
        return expr.test(EmailID);
    };
    $("#EmailID").change(function () {
        debugger
        if (!ValidateEmail($("#EmailID").val())) {
            $("#EmailID").focus();
            alert("Invalid EmailID address.");
            return false;
        }
    });
</script>
