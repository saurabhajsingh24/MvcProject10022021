<script>
    $(document).ready(function () {
        $("#RoleName,#RoleName2").autocomplete({
            source: function (request, response) {
                $.ajax({
                    url: '@Url.Action("GetRoleRecord", "Users")',
                    type: "POST",
                    dataType: "json",
                    data: { prefix: request.term },
                    success: function (data) {
                        response($.map(data, function (item) {
                            return {
                                label: item.RoleName, value: item.RoleName, id: item.RoleID
                            };
                        }))
                    }
                })
            },
            minLength: 1,
            change: function (event, ui) {
                if (!ui.item) {
                    this.value = '';
                }
            },
            select: function (event, i) {
                //var show = i.item.id;
                debugger
                var chk = $(this).attr("id");
                if (chk == "RoleName2") {
                    $("#RoleID2").val(i.item.id);
                }
                else {
                    $("#RoleID").val(i.item.id);
                }
                if ($("#RoleID").val() == 1) {
                    $("#adminrole").show();
                }
                else
                {
                    $("#adminrole").hide();
                    $("#RoleID2").val("");
                }
            },
        });
        $.ajax
            ({
                type: "POST",
                url: '@Url.Action("DatatableBind", "Users")',
                dataType: "json",

                success: function (result) {
                    var Srno = 1;
                    $.each(result, function (i, obj) {
                        $("#datatable-responsive").append('<tr><td>' + Srno + '</td><td style="display:none">' + obj.UserID + '</td><td>' + obj.FullName + '</td><td <td style="display:none">>' + obj.LoginName + '</td><td style="display:none">' + obj.EmailID + '</td><td style="display:none">' + obj.Password + '</td><td style="display:none">' + obj.Password + '</td><td <td style="display:none">>' + obj.RowInternal + '</td><td <td style="display:none">>' + obj.RoleName + '</td></tr>')
                        Srno++;
                    });

                    $('#datatable-responsive').DataTable({
                        dom: 'Bfrtip',
                        buttons: [{
                            extend: 'excel',
                            text: 'Excel',
                            className: 'exportExcel',
                            filename: 'Test_Excel',
                            exportOptions: { modifier: { page: 'all' } }
                        },
                        {
                            extend: 'csv',
                            text: 'CSV',
                            className: 'exportExcel',
                            filename: 'Test_Csv',
