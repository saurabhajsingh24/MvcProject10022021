        $(document).ready(function () {
            $('#datatable-responsive tbody').on('click', 'td', function () {
                debugger;
         
                $("#save").text("Update");
                $("save").addClass("btndivcss");
                var row_num = parseInt($(this).parent().index());
                var UserID = parseInt(document.getElementById("datatable-responsive").tBodies[0].rows[row_num].cells[1].innerHTML);
                var FullName = document.getElementById("datatable-responsive").tBodies[0].rows[row_num].cells[2].innerHTML;
                var LoginName = document.getElementById("datatable-responsive").tBodies[0].rows[row_num].cells[3].innerHTML;
                var EmailID = document.getElementById("datatable-responsive").tBodies[0].rows[row_num].cells[4].innerHTML;
                var Password = document.getElementById("datatable-responsive").tBodies[0].rows[row_num].cells[5].innerHTML;
                var RePassword = document.getElementById("datatable-responsive").tBodies[0].rows[row_num].cells[6].innerHTML;
                var RowInternal = document.getElementById("datatable-responsive").tBodies[0].rows[row_num].cells[7].innerHTML;
                var RoleName = document.getElementById("datatable-responsive").tBodies[0].rows[row_num].cells[8].innerHTML;

                $("#UserID").val(UserID);
                $("#FullName").val(FullName);
                $("#LoginName").val(LoginName);
                $("#EmailID").val(EmailID);
                $("#Password").val(Password);
                $("#RePassword").val(Password);
                $("#RowInternal").val(RowInternal);
                $("#RoleID").val(RoleID);

                $.ajax({
                        type: "GET",
                        url: '@Url.Action("Fill", "Users")',
                        data: { UserID: UserID },
                        success: function (result) {
                            $.each(result, function (i, obj) {
                                $("#UserID").val(obj.UserID);
                                $("#FullName").val(obj.FullName);
                                $("#LoginName").val(obj.LoginName);
                                $("#EmailID").val(obj.EmailID);
                                $("#Password").val(obj.Password);
                                $("#RePassword").val(obj.Password);
                                $("#RoleName").val(obj.RoleName);
                                $("#RoleID").val(obj.RoleID);
                                $("#UserDetailsID").val(obj.UserDetailsID);
                                debugger
                                if (obj.chbAuthorizationRights=="True")
                                {
                                    $("#divAuth").show();
                                     $("#chbAuthorizationRights").prop('checked', true);
                                    
                                }
                                else
                                {
                                    $("#divAuth").hide();
                                    $("#chbAuthorizationRights").prop('checked', false);
                                }
                               

                                if (obj.RoleID == 1) {
                                    $("#adminrole").show();
                                    $("#RoleName2").val(obj.RoleName2);
                                    $("#RoleID2").val(obj.RoleID2);
                                    $("#Password2").val(obj.Password2);
                                }
                                else {
                                    $("#adminrole").hide();
                                    $("#RoleName2").val("");
                                    $("#RoleID2").val("");
                                    $("#Password2").val("");
                                }
                               
                            });
                        }
                    })
            });
        });

        $.ajax
          ({
              type: "POST",
              url: '@Url.Action("GetAllHospitalLocation", "Users")',
              dataType: "json",

              success: function (result) {
                  var Srno = 1;
                  $.each(result, function (i, obj) {
                      $("#Hospital-table").append('<tr><td>' + Srno + '</td><td style="display:none">' + obj.HospitalID + '</td><td>' + obj.HospitalName + '</td></tr>')
                      Srno++;
                  });
              },
              error: function () {
              }
          });
    });

</script>

