<style>
    th {
        width: 33.33%;
    }

    thead {
        background: #d0e6f6;
        display: table;
        width: 100%;
        width: calc(100% - 8px);
    }

    tfoot {
        background: #d0e6f6;
        display: table;
        width: 100%;
        width: calc(100% - 8px);
        padding: 3px;
        border: 1px solid white !important;
    }

        tfoot td {
            border: 1px solid white !important;
            padding: 1px !important;
        }

    tbody {
        height: 180px;
        overflow: auto;
        overflow-x: hidden;
        display: block;
        width: 100%;
    }

        tbody tr {
            display: table;
            width: 100%;
            table-layout: fixed;
        }

    .btn-primary {
        background-color: #35a4f1 !important;
        color: #fff;
        border: 1px solid #35a4f1;
    }

    .file-field input[type=file] {
        position: absolute;
        top: 0;
        right: 0;
        left: 0;
        bottom: 0;
        width: 100%;
        margin: 0;
        padding: 0;
        cursor: pointer;
        opacity: 0;
    }
</style>
<style>
    .tab .nav-tabs > li {
        margin-right: 2px;
    }

        .tab .nav-tabs > li > a {
            border: none;
            padding: 10px 15px;
            color: #000;
            background: #bddff7;
            border-radius: 0;
            font-family: roboto;
        }

            .tab .nav-tabs > li > a > i {
                font-size: 14px;
                margin-right: 10px;
            }

        .tab .nav-tabs > li.active > a,
        .tab .nav-tabs > li.active > a:focus,
        .tab .nav-tabs > li.active > a:hover {
            border: none;
            background: #35a4f1;
            color: #fff;
            transition: background 0.20s linear;
        }

    a {
        color: #195580;
        text-decoration: none;
    }

    .tab .nav-tabs li.active:after {
        content: " ";
        position: absolute;
        bottom: -30px;
        left: 37%;
        border: 15px solid transparent;
        border-top-color: #35a4f1;
    }

    .tab .tab-content {
        background: #fdfdfd;
        line-height: 25px;
        border: 1px solid #ddd;
        border-top: 2px solid #35a4f1;
        border-bottom: 2px solid #35a4f1;
        padding: 30px 45px;
        font-family: roboto;
        line-height: 30px;
    }

    @@media only screen and (max-width 480px) {
        .tab > .nav-tabs li {
            width: 100%;
        }

        .tab .nav-tabs > li > a {
            padding: 20px;
            font-family: roboto;
        }

        .tab .nav-tabs > li.active:after {
            border: none;
        }
    }
</style>
