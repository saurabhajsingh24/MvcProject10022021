<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<style>
    .tab .nav-tabs > li > a {
        border: none;
        padding: 10px 15px;
        color: #000;
        background: #bddff7;
        border-radius: 0;
        font-family: roboto;
    }

    .tab .nav-tabs > li.active > a,
    .tab .nav-tabs > li.active > a:focus {
        border: none;
        background: #1a5e8e;
        color: #fff !important;
        transition: background 0.20s linear;
    }

    .nav > li > a:hover {
        color: #000 !important;
    }

    a {
        color: #195580;
        text-decoration: none;
    }

    .tab .nav-tabs li.active:after {
        content: " ";
        position: absolute;
        bottom: -30px;
        left: 37%;
        border: 15px solid transparent;
        border-top-color: #1a5e8e;
    }

    .tab .tab-content {
        background: #fdfdfd;
        line-height: 25px;
        border: 1px solid #ddd;
        border-top: 2px solid #1a5e8e;
        border-bottom: 2px solid #1a5e8e;
        padding: 30px 45px;
        font-family: roboto;
        line-height: 30px;
    }

    @@media only screen and (max-width 480px) {
        .tab > .nav-tabs li {
            width: 100%;
        }

        .tab .nav-tabs > li > a {
            padding: 20px;
            font-family: roboto;
        }

        .tab .nav-tabs > li.active:after {
            border: none;
        }
    }
</style>
