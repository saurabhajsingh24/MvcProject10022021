<script>
    $(document).ready(function () {
        debugger;
        $.ajax({
            type: "POST",
            url: '@Url.Action("GetDischargeSummarySetting", "MasterSetting")',
            dataType: "json",
            type: 'POST',
            data: JSON.stringify(""),
            traditional: true,
            contentType: 'application/json',
            success: function (result) {
                var count = 1;
                $.each(result, function (i, obj) {
                    debugger;
                    $("#datatableAccountDetail").append('<tr><td><input type="text" name="PatientDischargeSettingID" id="PatientDischargeSettingID" readonly class="form-control" value=' + obj.PatientDischargeSettingID + ' /></td><td><input type="text" name="Name" id="Name" readonly class="form-control" value=' + obj.Name + ' /></td><td><input type="text" name="PrintAs" id="PrintAs" class="form-control" value=' + obj.PrintAs + ' /></td><td><input type="text" name="Sequence" id="Sequence" class="form-control" value=' + obj.Sequence + ' /></td></tr>');
                    count++;
                });
            },
            error: function () {
            }
        });
    })
</script>
<script>
    $(document).ready(function () {
        $.ajax({
            url: '@Url.Action("GetAllFinancialYear", "MasterSetting")',
            type: "GET",
            data: {},
            dataType: "json",
            success: function (data) {
                debugger;
                var i = 0;
                $(data).each(function (index, d) {
                    debugger
                    $("#FinancialYear").append('<option value=' + d.FinancialYearID + '>' + d.FinancialYears + '</option>');
                    i++;
                });
                $("#FinancialYear").val('@Model.FinancialYearID');
            },
            error: function (data) {
            }
        });
    });
</script>
