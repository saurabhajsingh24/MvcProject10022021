<script>
    $(document).ready(function () {
        $("#City").autocomplete({
            source: function (request, response) {
                debugger
                $.ajax({
                    url: '@Url.Action("GetCityBind", "ProductSupplier")',
                    type: "POST",
                    dataType: "json",
                    data: { prefix: request.term },
                    success: function (data) {
                        debugger
                        response($.map(data, function (item) {
                            return {
                                label: item.City, value: item.City, id: item.CityID
                            };
                        }));
                    }
                })
            },
            minLength: 1,
            change: function (event, ui) {
                $("#CityID").val(ui.item.id);
            }
        });
    });
</script>
<script>
    $(function () {
        $("#City").change(function () {
            debugger
            var City = $("#City").val();
            $.ajax({
                type: "POST",
                url: '@Url.Action("AjaxMethod", "ProductSupplier")',
                data: '{City: ' + JSON.stringify(City) + '}',
                contentType: "application/json; charset=utf-8",
                success: function (searchList) {
                    debugger
                    $("#State").val(searchList[0]);
                    $("#Country").val(searchList[1]);
                    $("#StateID").val(searchList[2]);
                    $("#CountryID").val(searchList[3]);
                },
            });
        });
        $("#postingAccount").autocomplete({
            source: function (request, response) {
                $.ajax({
                    url: '@Url.Action("BindPostindAccount", "ProductSupplier")',
                    type: "POST",
                    dataType: "json",
                    data: { prefix: request.term },
                    success: function (data) {
                        debugger
                        response($.map(data, function (item) {
                            return {
                                label: item.AccountName, value: item.AccountName, id: item.AccountsID
                            };
                        }))
                    }
                })
            },
            minLength: 1,
            change: function (event, ui) {
                if (!ui.item) {
                    this.value = '';
                }
            },
            select: function (event, i) {
                var show = i.item.id;
                $("#AccountsID").val(show);
            },
        });
        $("#ProductCompanyName").autocomplete({
            source: function (request, response) {
                $.ajax({
                    url: '@Url.Action("BindCompanyName", "ProductSupplier")',
                    type: "POST",
                    dataType: "json",
                    data: { prefix: request.term },
                    success: function (data) {
                       
                        debugger
                        response($.map(data, function (item) {
                            debugger
                            return {
                                label: item.ProductCompanyName, value: item.ProductCompanyName, id: item.ProductCompanyNameID
                            };
                        }))
                    }
                })
            },
            minLength: 1,
            change: function (event, ui) {
                if (!ui.item) {
                    this.value = '';
                }
            },
            select: function (event, i) {
                var show = i.item.id;
                $("#ProductCompanyNameID").val(show);
            },
        });
        $('#ProductSupplierName').change(function () {
            $('#ShortName').val($('#ProductSupplierName').val());
        });
        $("#mobileNumber").keyup(function () {
            $("#mobileNumber").val(this.value.match(/[0-9]*/));
            var MobileNo = $("#mobileNumber").val();
        });

        $.ajax({
            type: "GET",
            url: '@Url.Action("ShowAllProductSupplier", "ProductSupplier")',
            dataType: "json",
            success: function (data) {
                debugger
                for (var i = 0; i < data.length; i++) {
                    var SrNo = i + 1;
                    $("#productSupplierTable").append(
                        '<tr><td>' + SrNo + '</td><td>' + data[i].ProductSupplierName + '</td><td>' + data[i].AccountName + '</td><td style="display:none">' + data[i].ProductSupplierID + '</td></tr>');
                };
                $('#productSupplierTable').DataTable({
                    "fixedHeader": true,
                    "paging": true,
                    "lengthChange": true,
                    "searching": true,
                    "ordering": true,
                    "info": true,
                    "autoWidth": true,
                    "responsive": false,
                });
                var buttons = new $.fn.dataTable.Buttons($('#productSupplierTable').DataTable(), {
                    buttons: [
                        {
                            extend: 'excel',
                            text: '<li class="fa fa-file-excel-o fa-lg" style="font-weight: 600;"></li><br>Excel'
                        }
                    ]
                }).container().appendTo($('#accountsExcelExport'));
            },
            error: function (result) {
                alert("Error grd bind");
            }
        });

        $('#addProductSupplierDetail').click(function () {
            debugger       

           
            ProductCompanyName1 = $("#ProductCompanyName").val();

            if (ProductCompanyName1 =="") {
              alert('First add Company Name');
            }
            else {

                cashDiscount1 = $("#cashDiscount").val();
                creditDiscount1 = $("#creditDiscount").val();
                lessBy1 = $("#lessBy").val();
                ProductCompanyNameID1 = $('#ProductCompanyNameID').val();

                $("#productSupplierDetailTable").append('<tr><td>' + '<input type="text" readonly value="' + ProductCompanyName1 + '"  class="form-control"  name="ProductCompanyName1">' + '<input type="hidden" readonly value="' + ProductCompanyNameID1 + '" name="ProductCompanyNameID1">' + '</td><td>' + '<input type="text" readonly value="' + cashDiscount1 + '" class="form-control" name="cashDiscount1">' + '</td><td>' + '<input type="text" readonly value="' + creditDiscount1 + '" class="form-control" name="creditDiscount1">' + '</td><td>' + '<input type="text" readonly value="' + lessBy1 + '"  class="form-control" name="lessBy1">' + '</td><td><a href="#" class="fa fa-trash-o"  id="delrow" class="delrow"></a></td></tr>');

                $("#ProductCompanyName").val("");
                $("#cashDiscount").val("");
                $("#creditDiscount").val("");
                $("#lessBy").val("");
            }
           
        });
       
        $("#productSupplierDetailTable").on('click', '#delrow', function () {
            debugger;
            $(this).closest('tr').remove();
            $(this).closest('tr').text();
        });

        $('#productSupplierTable').delegate('td', 'click', function () {
            $("#save").html("<i class='fa fa-save'></i>Update");
            debugger;
            var row_num = parseInt($(this).parent().index());
            var ProductSupplierID = parseInt(document.getElementById("productSupplierTable").tBodies[0].rows[row_num].cells[3].innerHTML);
           
            $.ajax({
                type: "GET",
                url: '@Url.Action("EditProductSupplier", "ProductSupplier")',
                data: { id: ProductSupplierID },
                dataType: "json",
                success: function (data) {
                    debugger;

                    $("#ProductSupplierName").val(data[0].ProductSupplierName);
                    $("#ShortName").val(data[0].ShortName);
                    $("#DLNo").val(data[0].DLNo);
                    $("#FoodLicNo").val(data[0].FoodLicNo);
                    $("#CreditDay").val(data[0].CreditDay);
                    $("#CSTNo").val(data[0].CSTNo);
                    $("#TINNO").val(data[0].TINNO);
                    $("#TINDate").val(data[0].TINDate);
                    $("#AccountsID").val(data[0].AccountsID);
                    $("#postingAccount").val(data[0].AccountName);
                    $("#ContactPerson").val(data[0].ContactPerson);
                    $("#Satus").val(data[0].Satus);
                    $("#Days").val(data[0].Days);
                    $("#Address").val(data[0].Address);                   
                    $("#PinCode").val(data[0].PinCode);
                    $("#City").val(data[0].CityName);
                    $("#State").val(data[0].StateName);
                    $("#Country").val(data[0].CountryName);
                    $("#CityID").val(data[0].CityID);
                    $("#StateID").val(data[0].StateID);
                    $("#CountryID").val(data[0].CountryID);
                    $("#EmailID").val(data[0].EmailID);
                    $("#PhoneNo").val(data[0].PhoneNo);
                    $("#MobileNo").val(data[0].MobileNo);
                    $("#Pan").val(data[0].Pan);
                    $("#ProductSupplierID").val(data[0].ProductSupplierID);
                    //$("#ProductSupplierDetailID").val(data[0].ProductSupplierDetailID);

                    $('#productSupplierDetailTable tbody tr').remove();
                    $.each(data, function (i, obd) {
                        debugger;
 
                        $("#productSupplierDetailTable").append('<tr><td>' + '<input type="text" readonly value="' + obd.ProductCompanyName1 + '"  class="form-control"  name="ProductCompanyName1">' + '<input type="hidden" readonly value="' + obd.ProductCompanyNameID1 + '" name="ProductCompanyNameID1">' + '</td><td>' + '<input type="text" readonly value="' + obd.cashDiscount1 + '" class="form-control" name="cashDiscount1">' + '</td><td>' + '<input type="text" readonly value="' + obd.creditDiscount1 + '" class="form-control" name="creditDiscount1">' + '</td><td>' + '<input type="text" readonly value="' + obd.lessBy1 + '"  class="form-control" name="lessBy1">' + '</td><td><a href="#" class="fa fa-trash-o"  id="delrow" class="delrow"></a></td></tr>');

                       
                    });
                },
            });
        })
    });
</script>

<script>
    function Delete(ProductSupplierID) {
        debugger
        var ans = confirm("Are you sure you want to Delete this Record?");
        if (ans) {
            $.ajax({
                type: "POST",
                url: '@Url.Action("DeleteProductSupplier", "ProductSupplier")',
                data: { ProductSupplierID: ProductSupplierID },
                dataType: "json",
                success: function (data) {
                    debugger
                    if (data == "ProductSupplier Deleted Successfully") {
                        alert("ProductSupplier Deleted Successfully");
                        window.location.reload(true);
                    }
                    else {
                        alert(data);
                    }
                },
                error: function (errormessage) {
                    alert(errormessage.responseText);
                }
            });
        }
    }
</script>
