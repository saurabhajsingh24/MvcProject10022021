<script>
    $(document).ready(function () {

        $("#City").autocomplete({

            source: function (request, response) {
                debugger
                $.ajax({

                    url: '@Url.Action("GetCityBind", "HospitalLocation")',
                    type: "POST",
                    dataType: "json",
                    data: { prefix: request.term },
                    success: function (data) {
                        debugger
                        response($.map(data, function (item) {
                            return {
                                label: item.City, value: item.City, id: item.CityID
                            };
                        }));
                    }
                })
            },

            minLength: 1,
            change: function (event, ui) {
                $("#CityID").val(ui.item.id);
            }

        });
    });
</script>


<script>
    $(function () {
        $("#City").change(function () {
            debugger
            var City = $("#City").val();
            $.ajax({
                type: "POST",

                url: '@Url.Action("AjaxMethod", "HospitalLocation")',
                data: '{City: ' + JSON.stringify(City) + '}',
                contentType: "application/json; charset=utf-8",

                success: function (searchList) {
                    debugger
                    $("#State").val(searchList[0]);
                    $("#Country").val(searchList[1]);

                    $("#StateID").val(searchList[2]);
                    $("#CountryID").val(searchList[3]);
                },
            });
        });
    });
</script>

