                $("#batchNumber").val("");
                $("#Expiry").val("");
                $("#mrp").val("0.00");
                $("#salesRate").val("0.00");
                $("#purchaseRate").val("0.00");
                $("#cgst").val("0.00");
                $("#sgst").val("0.00");
                $("#utgst").val("0.00");
                $("#gst").val("0.00");
                $("#cess").val("0.00");
                $("#productDate").val("");
                $("#productDetailID1").val("");
               
            }
            else
            {
                alert("% can not be greater than 100 OR less than 0 !");
            }

        }
    });
    });
</script>
