                                                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12 cpb-10">
                                                    <input type="text" id="extrafield" name="extrafield" class="form-control">
                                                </div>
                                                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12 cpb-10">
                                                    <div class="form-check-inline pull-right">
                                                        <label class="form-check-label">
                                                            <input type="checkbox" class="form-check-input" id="detailextrafield" name="detailextrafield">Detail with Extra Field
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                            <!--Discontinue Date-->
                                            <div class="form-group col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                                                    <label>Discontinue Date</label>
                                                </div>
                                                <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12 cpb-10">
                                                    <input type="date" id="discontinueDate" name="discontinueDate" value="@DateTime.Now.ToString("yyyy-MM-dd")" max="9999-12-31" class="form-control">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                <div class="card card-primary panel-primary">
                                    <h2 class="panelheading">Product Rates</h2>
                                    <div class="card-body pane-body">
                                        <div class="row">
                                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                <!--Bar Code-->
                                                <div class="form-group col-lg-2 col-md-12 col-sm-12 col-xs-12">
                                                    <label>Bar Code</label>
                                                    <input type="number" id="addbarCode" name="addbarCode" class="form-control">
                                                </div>
                                                <!--Batch Number-->
                                                <div class="form-group col-lg-2 col-md-12 col-sm-12 col-xs-12">
                                                    <label>Batch Number</label>
                                                    <input type="number" id="batchNumber" name="batchNumber" class="form-control">
                                                    <input type="hidden" id="productDetailID1" name="productDetailID1" class="form-control">
                                                </div>
                                                <!--Expiry-->
                                                <div class="form-group col-lg-2 col-md-12 col-sm-12 col-xs-12">
                                                    <label>Expiry</label>
                                                    <input type="date" id="Expiry" name="Expiry" value="@DateTime.Now.ToString("yyyy-MM-dd")" max="9999-12-31" class="form-control">
                                                </div>
                                                <!--M.R.P.-->
                                                <div class="form-group col-lg-2 col-md-12 col-sm-12 col-xs-12">
                                                    <label>M.R.P</label>
                                                    <input type="number" id="mrp" name="mrp" value="0.00" class="form-control">
                                                </div>
                                                <!--Sales Rate-->
                                                <div class="form-group col-lg-2 col-md-12 col-sm-12 col-xs-12">
                                                    <label>Sales Rate</label>
                                                    <input type="number" id="salesRate" name="salesRate" value="0.00" class="form-control">
                                                </div>
                                                <!--Purchase Rate-->
                                                <div class="form-group col-lg-2 col-md-12 col-sm-12 col-xs-12">
                                                    <label>Purchase Rate</label>
                                                    <input type="number" id="purchaseRate" name="purchaseRate" value="0.00" class="form-control">
                                                </div>
                                            </div>
                                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                <!--G.S.T-->
                                                <div class="form-group col-lg-2 col-md-12 col-sm-12 col-xs-12">
                                                    <label>G.S.T %</label>
                                                    <input type="text" id="gst" name="gst" value="0.00" class="form-control">
                                                </div>
                                                <!--S.G.S.T-->
                                                <div class="form-group col-lg-2 col-md-12 col-sm-12 col-xs-12">
                                                    <label>S.G.S.T %</label>
                                                    <input type="text" id="sgst" name="sgst" value="0.00" class="form-control">
                                                </div>
                                                <!--C.G.S.T-->
                                                <div class="form-group col-lg-2 col-md-12 col-sm-12 col-xs-12">
                                                    <label>C.G.S.T %</label>
                                                    <input type="text" id="cgst" name="cgst" value="0.00" class="form-control">
                                                </div>
                                                <!--U.T.G.S.T-->
                                                <div class="form-group col-lg-2 col-md-12 col-sm-12 col-xs-12">
                                                    <label>U.T.G.S.T %</label>
                                                    <input type="text" id="utgst" name="utgst" value="0.00" class="form-control">
                                                </div>
                                                <!--C.E.S.S-->
                                                <div class="form-group col-lg-2 col-md-12 col-sm-12 col-xs-12">
                                                    <label>C.E.S.S %</label>
                                                    <input type="text" id="cess" name="cess" value="0.00" class="form-control">
                                                </div>
                                                <!--Product Date-->
                                                <div class="form-group col-lg-2 col-md-12 col-sm-12 col-xs-12">
                                                    <label>Product Date</label>
                                                    <input type="date" id="productDate" name="productDate" value="@DateTime.Now.ToString("yyyy-MM-dd")" max="9999-12-31" class="form-control">
                                                </div>
                                            </div>
                                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                <!--O.P Stock U-->
                                                <div class="form-group col-lg-2 col-md-12 col-sm-12 col-xs-12">
                                                    <label>O.P Stock U</label>
                                                    <input type="number" id="opStockU" name="opStockU" value="0.00" class="form-control">
                                                </div>
                                                <!--O.P Stock L-->
                                                <div class="form-group col-lg-2 col-md-12 col-sm-12 col-xs-12">
                                                    <label>O.P Stock L</label>
                                                    <input type="number" id="opStockL" name="opStockL" value="0.00" class="form-control">
                                                </div>
                                                <!--Add Rates-->
                                                <div class="col-lg-2 col-md-12 col-sm-12 col-xs-12 pull-right" style="padding-top: 25px;">
                                                    <button type="button" id="addProductRates" name="addProductRates" class="btndivcss pull-right" style="height: 25px;">
                                                        Add Rates
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 table-responsive">
                                            <table id="productRatesTable" class="table table-bordered hover display dataTable table-hover" role="grid" style="width: 100%">
                                                <thead style="background-color: #bddff7;">
                                                    <tr>

                                                        <th>Batch Number</th>
                                                        <th>Expiry Date</th>
                                                        <th>M.R.P</th>
                                                        <th>Sales Rate</th>
                                                        <th>Purchase Rate</th>
                                                        <th>C.G.S.T</th>
                                                        <th>S.G.S.T</th>
                                                        <th>U.T.G.S.T</th>
                                                        <th>G.S.T</th>
                                                        <th>C.E.S.S</th>
                                                        <th>Product Date</th>
                                                        <th>Delete</th>
                                                    </tr>
                                                </thead>
                                                <tbody></tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12" style="text-align: center;">
                                <div class="btn-group dropdown-split-warning">
                                    <button type="submit" class="btn btn-app" id="save">
                                        <i class="fa fa-save"></i>Save
                                    </button>
                                </div>
                                <div class="btn-group dropdown-split-warning">
                                    <a class="btn btn-app" id="delete" onclick="Delete(document.getElementById('productNameID').value); location.reload();">
                                        <i class="fa fa-trash"></i>Delete
                                    </a>
                                </div>
                                <div class="btn-group dropdown-split-warning">
                                    <a class="btn btn-app" onclick="location.reload();">
                                        <i class="fa fa-refresh"></i>Clear
                                    </a>
                                </div>
                                <div class="btn-group dropdown-split-warning">
                                    <a class="btn btn-app" onclick="window.location.href='@Url.Action("PatientMaster", "Application")'">
                                        <i class="fa fa-sign-out"></i>Exit
                                    </a>
                                </div>
                                <div class="btn-group dropdown-split-warning">
                                    <a class="btn btn-app" id="productTableExcelExport"></a>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 table-responsive">
                                <table id="productTable" class="table table-bordered hover display dataTable table-hover" role="grid" style="width: 100%">
                                    <thead style="background-color: #bddff7;">
                                        <tr>
                                            <th>Sr.No.</th>
                                            <th>Product Name</th>
                                            <th>Product Company Name</th>
                                            <th>Packing</th>
                                            <th>Product Lower Unit Name</th>
                                            <th>Contain</th>
                                            <th style="display:none">Product Name ID</th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
        </form>
    </div>
</body>