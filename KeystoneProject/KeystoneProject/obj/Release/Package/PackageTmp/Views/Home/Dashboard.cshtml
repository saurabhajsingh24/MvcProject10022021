<style>
    .amChartsLegend amcharts-legend-div {
        margin-left: 100px;
    }

    .d-inline-block {
        display: inline-block !important;
        padding-left: 20px;
    }

    .widget-statstic-card span {
        border-radius: 30px;
        background-color: #c6d8ff;
        padding: 5px 20px;
        color: #4680ff;
        font-weight: 600;
        margin-right: 15px;
    }

    .card .card-header h5 {
        margin-bottom: 0;
        color: #505458;
        font-size: 17px;
        font-weight: 500;
        display: inline-block;
        margin-right: 10px;
        line-height: 1.4;
    }

    .text-c-blue {
        color: #4680ff;
    }

    .widget-statstic-card:hover .st-icon {
        font-size: 50px;
    }

    .widget-statstic-card .st-icon {
        color: #fff;
        font-size: 23px;
        padding: 40px 40px 20px 20px;
        border-radius: 50%;
        position: absolute;
        top: -30px;
        right: -30px;
        -webkit-transition: all 0.3s ease-in-out;
        transition: all 0.3s ease-in-out;
    }

    .bg-c-blue {
        background-color: #4680ff;
    }

    .card .card-header .card-header-left {
        display: inline-block;
    }

    .card .card-header {
        background-color: transparent;
        border-bottom: none;
        padding: 25px 20px;
    }

    .card-header:first-child {
        border-radius: calc(.25rem - 1px) calc(.25rem - 1px) 0 0;
    }

    .card-header {
        padding: .75rem 1.25rem;
        margin-bottom: 0;
        background-color: rgba(0,0,0,.03);
        border-bottom: 1px solid rgba(0,0,0,.125);
    }

    .widget-statstic-card {
        position: relative;
        overflow: hidden;
    }

    .card {
        border-radius: 5px;
        -webkit-box-shadow: 0 1px 11px 0 rgba(0, 0, 0, 0.12);
        box-shadow: 0 1px 11px 0 rgba(0, 0, 0, 0.12);
        border: none;
        margin-bottom: 30px;
    }

    .card {
        position: relative;
        display: -ms-flexbox;
        display: flex;
        -ms-flex-direction: column;
        flex-direction: column;
        min-width: 0;
        word-wrap: break-word;
        background-color: #fff;
        background-clip: border-box;
        border: 1px solid rgba(0,0,0,.125);
        border-radius: .25rem;
    }

    .text-muted {
        color: #6c757d !important;
        font-size: 14px;
    }

    .widget-card-1 {
        margin-top: 10px;
        text-align: right;
    }

    .card .card-block-small {
        padding: 15px 20px;
    }

    .widget-card-1 .card1-icon {
        width: 60px;
        height: 60px;
        position: absolute;
        top: -15px;
        font-size: 35px;
        border-radius: 8px;
        display: -webkit-box;
        display: -ms-flexbox;
        display: flex;
        color: #fff;
        -webkit-box-align: center;
        -ms-flex-align: center;
        align-items: center;
        -webkit-box-pack: center;
        -ms-flex-pack: center;
        justify-content: center;
        -webkit-transition: all 0.3s ease-in-out;
        transition: all 0.3s ease-in-out;
    }

    .text-c-blue {
        color: #4680ff;
    }

    .widget-card-1 h4 {
        font-weight: 400;
        margin-top: 10px;
    }

    text-muted {
        color: #919aa3 !important;
    }

    .bg-c-pink {
        background-color: #FC6180;
    }

    .bg-c-green {
        background-color: #93BE52;
    }

    .bg-c-yellow {
        background-color: #FFB64D;
    }

    .text-c-pink {
        color: #FC6180;
    }

    .text-c-green {
        color: #93BE52;
    }

    .text-c-yellow {
        color: #FFB64D;
    }

    .widget-card-1:hover .card1-icon {
        top: -25px;
    }

    body {
        font-size: 16px;
        font-family: roboto;
    }

    .card .card-header {
        padding: 10px 20px !important;
    }
</style>


