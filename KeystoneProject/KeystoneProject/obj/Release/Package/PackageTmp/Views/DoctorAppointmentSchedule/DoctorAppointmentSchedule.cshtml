        $(function () {

            var d = new Date(),
                h = d.getHours(),
                m = d.getMinutes();
            if (h < 10) h = '0' + h;
            if (m < 10) m = '0' + m;
            $('input[type="time"][value="now"]').each(function () {
                $(this).attr({ 'value': h + ':' + m });

            });
        });
        function CaculateFromToTimeMin() {
            debugger
            var FromTime = $("#FromTime").val();
            var ToTime = $("#ToTime").val();
            var hour1 = FromTime.substring(0, 2);
            var hour2 = ToTime.substring(0, 2);
            if (parseInt(hour1) != parseInt(hour2)) {
                if (parseInt(hour1) < parseInt(hour2)) {
                    var min1 = FromTime.substring(3, 5);
                    var min2 = ToTime.substring(3, 5);
                    var Total_Hor = hour1 - hour2;
                    var Total_Min = min1 - min2;
                    var total = Total_Hor * 60;
                    var Total_Time = total + Total_Min;
                    var d = String(Total_Time).replace("-", "");
                    var tot = d / $("#NoOfAppointment").val();
                    $("#PerAppointmentTimeinMinute").val(tot);
                }
                else {
                    //alert("select Valid Time");
                    $("#PerAppointmentTimeinMinute").val("");
                    $("#NoOfAppointment").val("");
                }
            }
            else {
                alert("select Valid Time");
                $("#PerAppointmentTimeinMinute").val("");
                $("#NoOfAppointment").val("");
            }
        }
    })

