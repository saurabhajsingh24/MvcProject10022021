                                    <a class="btn btn-app" id="Delete" onclick="Delele(document.getElementById('DoctorAppoinmentScheduleID').value)">
                                        <i class="fa fa-trash"></i>Delete
                                    </a>
                                </div>
                                <div class="btn-group dropdown-split-warning">
                                    <a class="btn btn-app" onclick="location.reload();">
                                        <i class="fa fa-refresh"></i>Clear
                                    </a>
                                </div>
                                <div class="btn-group dropdown-split-warning">
                                    <a class="btn btn-app" onclick="window.location.href='@Url.Action("patientmaster", "Application")'">
                                        <i class="fa fa-sign-out"></i>Exit
                                    </a>
                                </div>
                                <div class="btn-group dropdown-split-warning">
                                    <a class="btn btn-app" id="doctorAppointmentScheduleExcelExport"></a>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                        <div class=" col-lg-9 col-md-12 col-sm-12 col-xs-12 margin-r-5">
                                            <div class="table-responsive">
                                                <table id="doctorAppointmentScheduleTableId" class="table table-bordered hover display dataTable table-hover" role="grid" aria-describedby="example1_info"
                                                       style="width: 100%">
                                                    <thead style="background-color: #bddff7;">
                                                        <tr>
                                                            <th scope="col">Sr. no.</th>
                                                            <th scope="col">Dr. Name</th>
                                                            <th scope="col">Appt. Type</th>
                                                            <th scope="col">Appt. Date</th>
                                                            <th scope="col">From Time</th>
                                                            <th scope="col">To Time</th>
                                                            <th scope="col">No of Appt.</th>
                                                            <th style="display:none">DoctorID</th>
                                                            <th style="display:none">DoctorAppID</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody class="scrollbar">
                                                        @{
                                                            for (int i = 0; i < Model.StoreAllDoctorAppointment.Tables[0].Rows.Count; i++)
                                                            {
                                                                var SrNo = i + 1;
                                                                var DoctorAppoinmentScheduleID = Model.StoreAllDoctorAppointment.Tables[0].Rows[i]["DoctorAppoinmentScheduleID"].ToString();
                                                                var DoctorID = Model.StoreAllDoctorAppointment.Tables[0].Rows[i]["DoctorID"].ToString();
                                                                var DoctorPrintName = Model.StoreAllDoctorAppointment.Tables[0].Rows[i]["DoctorPrintName"].ToString();
                                                                var AppointmentType = Model.StoreAllDoctorAppointment.Tables[0].Rows[i]["AppointmentType"].ToString();
                                                                DateTime FromDate = (DateTime)Model.StoreAllDoctorAppointment.Tables[0].Rows[i]["FromDate"];
                                                                var FromTime = Convert.ToDateTime(Model.StoreAllDoctorAppointment.Tables[0].Rows[i]["FromTime"]).ToString("hh:mm tt");
                                                                var ToTime = Convert.ToDateTime(Model.StoreAllDoctorAppointment.Tables[0].Rows[i]["ToTime"]).ToString("hh:mm tt");
                                                                var NoOfAppointment = Model.StoreAllDoctorAppointment.Tables[0].Rows[i]["NoOfAppointment"].ToString();
                                                                <tr role="row" class="odd">
                                                                    <td tabindex="0" class="sorting_1" style="text-transform: uppercase; text-align: center;width:10px">@SrNo</td>
                                                                    <td>@DoctorPrintName</td>
                                                                    <td>@AppointmentType</td>
                                                                    <td>@FromDate.ToShortDateString()</td>
                                                                    <td>@FromTime</td>
                                                                    <td>@ToTime</td>
                                                                    <td style="width:110px">@NoOfAppointment</td>
                                                                    <td style="display:none">@DoctorID</td>
                                                                    <td style="display:none">@DoctorAppoinmentScheduleID</td>
                                                                </tr>
                                                            }
                                                        }
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>

                                        <div class=" col-lg-3 col-md-12 col-sm-12 col-xs-12" style="border: 1px solid #cfcfdb;border-radius: 4px;padding: 10px;">
                                            <span style="font-size: 17px;"> Doctor Schedule Details</span>
                                            <div class="table-responsive">
                                                <table id="doctorScheduleDetailsTableId" class="table table-bordered hover displays table-hover" style="width: 100%">
                                                    <thead>
                                                        <tr style="background: #bddff7;">
                                                            <th scope="col">Appt. Type</th>
                                                            <th scope="col">Doctor Name</th>
                                                            <th scope="col">From Time</th>
                                                            <th scope="col">To Time</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody class="scrollbar"></tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                </section>
            </form>
        </div>
    </div>
</body>
