    <style>

        @@media (min-width: 992px) {
            .sidebar-mini.sidebar-collapse .main-sidebar, .sidebar-mini.sidebar-collapse .main-sidebar::before {
                margin-left: 0;
                width: 7.6rem;
            }
        }

        @@media (min-width: 992px) {
            .sidebar-mini.sidebar-collapse .content-wrapper,
            .sidebar-mini.sidebar-collapse .main-footer, .sidebar-mini.sidebar-collapse .main-header {
                margin-left: 7rem !important;
            }
        }

        .elevation-4 {
            box-shadow: 0 0px 0px rgba(0,0,0,0),0 0px 0px rgba(0,0,0,0) !important;
        }

        .close {
            float: right;
            font-size: 21px;
            font-weight: 200;
            line-height: 1;
            color: #fff;
            text-shadow: 0 1px 0 #fff;
            filter: alpha(opacity=20);
            opacity: .9;
        }

        .modal-header .close {
            margin-top: -6px;
        }

        .modal-header {
            border-bottom: 1px solid #e5e5e5;
            background: #0f9bfa;
        }

        .modal-title {
            color: #ffffff;
        }

        .nav-sidebar .menu-open > .nav-link i.right {
            -webkit-transform: rotate(ss90deg);
            transform: rotate(90deg);
        }

        .nav-sidebar > .nav-item .nav-icon.fa {
            font-size: 19px;
        }

        .nav-sidebar > .nav-item .nav-icon {
            margin-right: 1rem;
        }

        /*.sidebar-dark-primary {
            background-color: #2876ad;
        }*/

        .sidebar-dark-primary .nav-sidebar > .nav-item.menu-open > .nav-link, .sidebar-dark-primary .nav-sidebar > .nav-item:hover > .nav-link, .sidebar-dark-primary .nav-sidebar > .nav-item > .nav-link:focus {
            /*background-color: rgb(252, 97, 128);
            color: #ffffff;*/
            background-color: #1a5e8e;
            color: #ffffff !important;
        }

        .sidebar-dark-primary .sidebar a {
            color: #000000 !important;
        }

        .sidebar-dark-primary {
            background-color: #bddff7;
        }

        [class*=sidebar-dark-] .nav-treeview > .nav-item > .nav-link:focus, [class*=sidebar-dark-] .nav-treeview > .nav-item > .nav-link:hover {
            background-color: #61afe68f;
        }

    </style>

    <style>
        select.input-sm {
