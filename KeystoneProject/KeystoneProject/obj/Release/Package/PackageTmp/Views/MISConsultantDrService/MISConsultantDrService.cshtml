<script>
    $(document).ready(function () {
        $("#Name").autocomplete({
            source: function (request, response) {
                $.ajax({
                    url: '@Url.Action("GetAllPatient", "MISConsultantDrService")',
                    type: "POST",
                    dataType: "json",
                    data: { prefix: request.term },
                    success: function (data) {
                        // $("#CategoryName").empty().append('<option selected="selected" value="0">All</option>');
                        response($.map(data, function (item) {
                            return {
                                label: item.PatientName, value: item.PatientName, id: item.PatientID,
                            };
                        }))
                    }
                })
            },
            minlength: 1,
            change: function (event, ui) {
                if (!ui.item) {
                    this.value = '';
                }
            },
            select: function (event, i) {
                var show = i.item.id;
                $("#ID").val(show);
                //alert($("#ID").val());
                $.ajax({
                    url: '@Url.Action("GetPreBalanceAmount", "MISConsultantDrService")',
                    type: "POST",
                    dataType: "json",
                    data: { id: $("#ID").val() },
                    success: function (data) {
                        // $("#CategoryName").empty().append('<option selected="selected" value="0">All</option>');
                        for (var d = 0; d < data.length; d++) {
                            $("#Name").val(data[d].PatientName);
                            $("#ID").val(data[d].PatientID);
                        };
                    }
                })
                //  SerGroupID = show;
            },
        });
    });
</script>
